import json

def filter_template(input_file, output_file):
    # 读取输入文件
    with open(input_file, 'r', encoding='utf-8') as f:
        data = json.load(f)
    
    # 筛选出 image_path 包含 '11234' 或 '18310' 的条目
    filtered_data = [item for item in data if '11234' in item.get('image_path', '') or '18310' in item.get('image_path', '')]

    # 将筛选后的数据写入输出文件
    with open(output_file, 'w', encoding='utf-8') as f:
        json.dump(filtered_data, f, ensure_ascii=False, indent=4)

# 示例用法
filter_template(input_file='template.json', output_file='filtered_template.json')
