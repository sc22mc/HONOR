git lfs track "*.png" "*.jpg" "*.jpeg" "*.webp" "*.bmp" "*.tif" "*.tiff" \
               "*.mp4" "*.avi" "*.mov" "*.mkv" \
               "*.zip" "*.tar" "*.tar.gz" "*.gz" "*.7z" \
               "*.npy" "*.npz" "*.pt" "*.pth" "*.bin" "*.ckpt" "*.safetensors" \
               "*.parquet" "*.h5"
git add .gitattributes
git commit -m "track large files with Git LFS"


git lfs migrate import --everything --above=50MB
